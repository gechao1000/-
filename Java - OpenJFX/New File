docker volume create m2

docker inspect m2

copy settings.xml to read dir

--- Dockerfile

FROM maven
WORKDIR /app

COPY pom.xml .
COPY src/ ./src/

CMD ["bash"]

---

docker build -t myapp . (just copy pom.xml src/)

docker run -it --rm -v m2:/root/.m2 -v "$(pwd)":/dist myapp


---

docker system prune